package model;

import java.time.LocalDateTime;

import jakarta.persistence.Entity;
import jakarta.persistence.Table;

@Entity
@Table(name = "vsp_organization")
public class Organization {
	private Long organizationKey; // Unique identifier for the organization
	private String organizationCode; // Code representing the organization
	private Boolean isHubOrganization; // Indicates if this is a hub organization
	private Boolean isNode; // Indicates if this organization is a node
	private Boolean isEnterprise; // Indicates if this organization is an enterprise
	private String organizationName; // Name of the organization
	private Boolean isBuyer; // Indicates if this organization is a buyer
	private Boolean isSeller; // Indicates if this organization is a seller
	private Boolean isCarrier; // Indicates if this organization is a carrier
	private String reference; // Additional reference information
	private LocalDateTime createTs; // Creation Timestamp
	private LocalDateTime modifyTs; // Modification Timestamp
	private String createUserId; // Creator User ID
	private String modifyUserId; // Modifier User ID
	private String createProgId; // Creator Program ID
	private String modifyProgId; // Modifier Program ID
}
